{
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "ESNEXT",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": false,
    "useDefineForClassFields": false,
    "skipLibCheck": true
  },
  "types": [
    "jest"
  ]
}
