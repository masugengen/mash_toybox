  "engines": {
    "node": "16.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "keywords": [],
  "license": "UNLICENSED",
  "lint-staged": {
    "*.scss": "npm run lint:scss",
    "*.ts": "npm run lint:ts",
    "*.ejs": "npm run lint:ejs"
  },
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/masugengen/mash_toybox"
  },
  "scripts": {
    "build": "npm run clean && npm run copy && run-s build:*",
    "build:ejs": "eleventy",
    "build:ts": "webpack --env resource=producttypescript --node-env=production",
    "build:prod": "webpack --mode=production --node-env=production",
    "build:scss": "gulp",
    "clean": "del-cli --force ./public",
    "copy": "node copy.js",
    "deploy": "node deploy",
    "dev": "webpack --mode=development",
    "dev:ts": "webpack --env resource=typescript --mode=development",
    "lint-staged": "lint-staged",
    "lint:scss": "stylelint **/*.scss",
    "lint:ejs": "ejslint **/*.ejs",
    "lint:ts": "eslint **/*.ts",
    "lint:vlint:html": "npx vlint --html",
    "lint:vlint:css": "npx vlint --css",
    "lint:vlint": "run-s lint:vlint:*",
    "predeploy": "npm run build",
    "prepare": "husky install",
    "start": "npm run clean && npm run copy && npm run watch",
    "takeout": "node takeout",
    "test": "jest",
    "watch": "run-p watch:*",
    "watch:assets": "webpack --mode=development --watch",
    "watch:ejs": "eleventy --watch",
    "watch:scss": "gulp && gulp watch",
    "watch:static": "node copyWatch.js",
    "watch:server": "browser-sync start --config bs-config.js"
  },
  "dependencies": {
    "@splidejs/splide": "^4.0.7",
    "swiper": "^8.2.5"
  }
}
